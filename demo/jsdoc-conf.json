{
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "name": "Doc Template",
    "footerText": "NHN Entertainment Frontend Development Lab",
    "versionSwitcher": {
        "versions": ["1.0.0", ">=1.0.3"],
        "excludeLevel": "patch"
    }
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "package": "package.json",
    "tutorials": "demo/samples"
  }
}
